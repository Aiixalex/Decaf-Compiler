; ModuleID = 'Test'
source_filename = "DecafComp"

declare void @print_int(i32)

define void @test(i1 %a) {
entry:
  %a1 = alloca i1
  store i1 %a, i1* %a1
  br label %ifstart

ifstart:                                          ; preds = %entry
  %a2 = load i1, i1* %a1
  br i1 %a2, label %iftrue, label %iffalse

iftrue:                                           ; preds = %ifstart
  call void @print_int(i32 1)
  br label %iffalse

iffalse:                                          ; preds = %iftrue, %ifstart
  br label %ifstart3

ifstart3:                                         ; preds = %iffalse
  %a6 = load i1, i1* %a1
  %nottmp = xor i1 %a6, true
  br i1 %nottmp, label %iftrue4, label %iffalse5

iftrue4:                                          ; preds = %ifstart3
  call void @print_int(i32 2)
  br label %iffalse5

iffalse5:                                         ; preds = %iftrue4, %ifstart3
  ret void
}

define i32 @main() {
entry:
  call void @test(i1 true)
  call void @test(i1 false)
  ret i32 0
}
