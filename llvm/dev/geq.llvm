; ModuleID = 'Test'
source_filename = "DecafComp"

declare void @print_int(i32)

define i32 @main() {
entry:
  %x = alloca i32
  store i32 0, i32* %x
  br label %ifstart

ifstart:                                          ; preds = %entry
  br i1 true, label %iftrue, label %iffalse

iftrue:                                           ; preds = %ifstart
  call void @print_int(i32 0)
  call void @print_int(i32 1)
  call void @print_int(i32 1)
  br label %iffalse

iffalse:                                          ; preds = %iftrue, %ifstart
  ret i32 0
}
